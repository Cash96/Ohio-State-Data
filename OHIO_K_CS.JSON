                  "objectiveID": "CS.K",
                  "objectiveTitle": "COMPUTING SYSTEMS",
                  "objectiveDescription": "Kindergarten standards for Computing Systems.",
                  "expectations": [
                    {
                      "expectationID": "CS.D.K.a",
                      "expectationDescription": "With guidance, identify and label commonly used devices and their components, explaining their connection to different tasks, to perform a variety of tasks."
                    },
                    {
                      "expectationID": "CS.HS.K.a",
                      "expectationDescription": "With guidance and support, identify and use hardware and software necessary for accomplishing a task."
                    },
                    {
                      "expectationID": "CS.T.K.a",
                      "expectationDescription": "With guidance and support, use problem-solving strategies to troubleshoot a problem."
                    }
                  ]
                },
                {
                  "objectiveID": "NI.K",
                  "objectiveTitle": "NETWORKS AND THE INTERNET",
                  "objectiveDescription": "Kindergarten standards for Networks and the Internet.",
                  "expectations": [
                    {
                      "expectationID": "NI.N.K.a",
                      "expectationDescription": "With guidance and support, create a list of ways information can be shared electronically to gain a deeper understanding of how information is transmitted (e.g., email, social media)."
                    },
                    {
                      "expectationID": "NI.C.K.a",
                      "expectationDescription": "With guidance and support, identify and use secure practices (e.g., passwords) to protect private information."
                    },
                    {
                      "expectationID": "NI.IOT.K.a",
                      "expectationDescription": "With guidance and support, identify what smart devices are to recognize what devices are able to connect to the internet."
                    },
                    {
                      "expectationID": "NI.IOT.K.b",
                      "expectationDescription": "With guidance and support, recognize how devices connect and exchange data over the internet to demonstrate how information is shared."
                    }
                  ]
                },
                {
                  "objectiveID": "DA.K",
                  "objectiveTitle": "DATA AND ANALYSIS",
                  "objectiveDescription": "Kindergarten standards for Data and Analysis.",
                  "expectations": [
                    {
                      "expectationID": "DA.DCS.K.a",
                      "expectationDescription": "Identify data to collect and sort."
                    },
                    {
                      "expectationID": "DA.DCS.K.b",
                      "expectationDescription": "With guidance and support, demonstrate how data can be collected and stored in a variety of ways."
                    },
                    {
                      "expectationID": "DA.VC.K.a",
                      "expectationDescription": "With guidance, organize and present data in various formats to make observations."
                    },
                    {
                      "expectationID": "DA.IM.K.a",
                      "expectationDescription": "With guidance, create a model of an object or process to identify patterns."
                    }
                  ]
                },
                {
                  "objectiveID": "ATP.K",
                  "objectiveTitle": "ALGORITHMIC THINKING AND PROGRAMMING",
                  "objectiveDescription": "Kindergarten standards for Algorithmic Thinking and Programming.",
                  "expectations": [
                    {
                      "expectationID": "ATP.A.K.a",
                      "expectationDescription": "With guidance and support, model a real-world process by constructing and following step-by-step directions (i.e., algorithms) to complete tasks."
                    },
                    {
                      "expectationID": "ATP.VDR.K.a",
                      "expectationDescription": "Recognize that a group of items (e.g., numbers, symbols or pictures) can be used to represent data."
                    },
                    {
                      "expectationID": "ATP.CS.K.a",
                      "expectationDescription": "With guidance and support, model a sequence of instructions (i.e., program) with a beginning, middle and end to solve a problem or express an idea."
                    },
                    {
                      "expectationID": "ATP.PD.K.a",
                      "expectationDescription": "With guidance and support, plan or create an artifact to illustrate thoughts, ideas and problems in a sequential (step-by-step) manner (e.g., story map, storyboard, sequential graphic organizer)."
                    }
                  ]
                },
                {
                  "objectiveID": "AI.K",
                  "objectiveTitle": "ARTIFICIAL INTELLIGENCE",
                  "objectiveDescription": "Kindergarten standards for Artificial Intelligence.",
                  "expectations": [
                    {
                      "expectationID": "AI.P.K.a",
                      "expectationDescription": "With guidance and support, locate sensors on computers, robots and intelligent appliances to understand that devices use sensors to gather information."
                    },
                    {
                      "expectationID": "AI.P.K.b",
                      "expectationDescription": "With guidance and support, access intelligent agents to demonstrate how they work."
                    },
                    {
                      "expectationID": "AI.RR.K.a",
                      "expectationDescription": "With guidance and support, use a simple decision tree to make a decision to visually and explicitly represent decisions and decision-making."
                    },
                    {
                      "expectationID": "AI.ML.K.a",
                      "expectationDescription": "With guidance and support, use a classifier that recognizes drawings to see if a program can guess what they are drawing."
                    },
                    {
                      "expectationID": "AI.NI.K.a",
                      "expectationDescription": "With guidance and support, identify how computers can recognize different humans using some form of recognition software to see how computers interact with humans."
                    },
                    {
                      "expectationID": "AI.SI.K.a",
                      "expectationDescription": "With guidance and support, locate AI applications used in daily life to understand how humans use AI."
                    },
                    {
                      "expectationID": "AI.SI.K.b",
                      "expectationDescription": "Discuss if artificial intelligence is good or bad (see impacts of computing)."
                    }
                  ]
                },
                {
                  "objectiveID": "IC.K",
                  "objectiveTitle": "IMPACTS OF COMPUTING",
                  "objectiveDescription": "Kindergarten standards for Impacts of Computing.",
                  "expectations": [
                    {
                      "expectationID": "IC.Cu.K.a",
                      "expectationDescription": "With guidance and support, identify technologies that impact one's own everyday life."
                    },
                    {
                      "expectationID": "IC.Cu.K.b",
                      "expectationDescription": "With guidance and support, recognize different ways computing devices are used regularly to understand technology's impact on oneâ€™s own daily life."
                    },
                    {
                      "expectationID": "IC.SI.K.a",
                      "expectationDescription": "With guidance and support, identify and use safe and responsible behaviors concerning information and technology."
                    },
                    {
                      "expectationID": "IC.SLE.K.a",
                      "expectationDescription": "With guidance, discuss appropriate uses of technology to support informed decisions."
                    },
                    {
                      "expectationID": "IC.SLE.K.b",
                      "expectationDescription": "With guidance and support, discuss examples of appropriate and inappropriate behavior online, including cyberbullying, and the steps to keep yourself and others safe and out of harm's way."
                    }
                  ]
                }
