              "objectives": [
                {
                  "objectiveID": "K.CC",
                  "objectiveTitle": "Counting and Cardinality",
                  "objectiveDescription": "Know number names and the count sequence.",
                  "expectations": [
                    {
                      "expectationID": "1",
                      "expectationDescription": "Count to 100 by ones and by tens."
                    },
                    {
                      "expectationID": "2",
                      "expectationDescription": "Count forward within 100 beginning from any given number other than 1."
                    },
                    {
                      "expectationID": "3",
                      "expectationDescription": "Write numerals from 0 to 20. Represent a number of objects with a written numeral 0-20 (with 0 representing a count of no objects)."
                    },
                    {
                      "expectationID": "4",
                      "expectationDescription": "Understand the relationship between numbers and quantities; connect counting to cardinality using a variety of objects including pennies."
                    },
                    {
                      "expectationID": "5",
                      "expectationDescription": "Count to answer 'how many?' questions about as many as 20 things arranged in a line, a rectangular array, or a circle, or as many as 10 things in a scattered configuration; given a number from 1-20, count out that many objects."
                    },
                    {
                      "expectationID": "6",
                      "expectationDescription": "Orally identify (without using inequality symbols) whether the number of objects in one group is greater/more than, less/fewer than, or the same as the number of objects in another group, not to exceed 10 objects in each group."
                    },
                    {
                      "expectationID": "7",
                      "expectationDescription": "Compare (without using inequality symbols) two numbers between 0 and 10 when presented as written numerals."
                    }
                  ]
                },
                {
                  "objectiveID": "K.OA",
                  "objectiveTitle": "Operations and Algebraic Thinking",
                  "objectiveDescription": "Understand addition as putting together and adding to, and understand subtraction as taking apart and taking from.",
                  "expectations": [
                    {
                      "expectationID": "1",
                      "expectationDescription": "Represent addition and subtraction with objects, fingers, mental images, drawings, sounds such as claps, acting out situations, verbal explanations, expressions, or equations."
                    },
                    {
                      "expectationID": "2",
                      "expectationDescription": "Solve addition and subtraction problems (written or oral), and add and subtract within 10 by using objects or drawings to represent the problem."
                    },
                    {
                      "expectationID": "3",
                      "expectationDescription": "Decompose numbers and record compositions for numbers less than or equal to 10 into pairs in more than one way by using objects and, when appropriate, drawings or equations."
                    },
                    {
                      "expectationID": "4",
                      "expectationDescription": "For any number from 1 to 9, find the number that makes 10 when added to the given number, e.g., by using objects or drawings, and record the answer with a drawing or, when appropriate, an equation."
                    },
                    {
                      "expectationID": "5",
                      "expectationDescription": "Fluently add and subtract within 5."
                    }
                  ]
                },
                {
                  "objectiveID": "K.NBT",
                  "objectiveTitle": "Number and Operations in Base Ten",
                  "objectiveDescription": "Work with numbers 11â€“19 to gain foundations for place value.",
                  "expectations": [
                    {
                      "expectationID": "1",
                      "expectationDescription": "Compose and decompose numbers from 11 to 19 into a group of ten ones and some further ones by using objects and, when appropriate, drawings or equations; understand that these numbers are composed of a group of ten ones and one, two, three, four, five, six, seven, eight, or nine ones."
                    }
                  ]
                },
                {
                  "objectiveID": "K.MD",
                  "objectiveTitle": "Measurement and Data",
                  "objectiveDescription": "Identify, describe, and compare measurable attributes.",
                  "expectations": [
                    {
                      "expectationID": "1",
                      "expectationDescription": "Identify and describe measurable attributes (length, weight, and height) of a single object using vocabulary terms such as long/short, heavy/light, or tall/short."
                    },
                    {
                      "expectationID": "2",
                      "expectationDescription": "Directly compare two objects with a measurable attribute in common to see which object has 'more of' or 'less of' the attribute, and describe the difference."
                    },
                    {
                      "expectationID": "3",
                      "expectationDescription": "Classify objects into given categories; count the numbers of objects in each category and sort the categories by count. The number of objects in each category should be less than or equal to ten. Counting and sorting coins should be limited to pennies."
                    }
                  ]
                },
                {
                  "objectiveID": "K.G",
                  "objectiveTitle": "Geometry",
                  "objectiveDescription": "Identify and describe shapes (squares, circles, triangles, rectangles, hexagons, cubes, cones, cylinders, and spheres).",
                  "expectations": [
                    {
                      "expectationID": "1",
                      "expectationDescription": "Describe objects in the environment using names of shapes, and describe the relative positions of these objects using terms such as above, below, beside, in front of, behind, and next to."
                    },
                    {
                      "expectationID": "2",
                      "expectationDescription": "Correctly name shapes regardless of their orientations or overall size."
                    },
                    {
                      "expectationID": "3",
                      "expectationDescription": "Identify shapes as two-dimensional (lying in a plane, 'flat') or three-dimensional ('solid')."
                    },
                    {
                      "expectationID": "4",
                      "expectationDescription": "Describe and compare two- or three-dimensional shapes, in different sizes and orientations, using informal language to describe their commonalities, differences, parts, and other attributes."
                    },
                    {
                      "expectationID": "5",
                      "expectationDescription": "Model shapes in the world by building shapes from components, e.g., sticks and clay balls, and drawing shapes."
                    },
                    {
                      "expectationID": "6",
                      "expectationDescription": "Combine simple shapes to form larger shapes."
                    }
                  ]
                }
              ],
