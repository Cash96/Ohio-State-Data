              "objectives": [
                {
                  "objectiveID": "1.OA",
                  "objectiveTitle": "Operations and Algebraic Thinking",
                  "objectiveDescription": "Represent and solve problems involving addition and subtraction.",
                  "expectations": [
                    {
                      "expectationID": "1.OA.1",
                      "expectationDescription": "Use addition and subtraction within 20 to solve word problems involving situations of adding to, taking from, putting together, taking apart and comparing, with unknowns in all positions, e.g., by using objects, drawings, and equations with a symbol for the unknown number to represent the problem."
                    },
                    {
                      "expectationID": "1.OA.2",
                      "expectationDescription": "Solve word problems that call for addition of three whole numbers whose sum is less than or equal to 20, e.g., by using objects, drawings, and equations with a symbol for the unknown number to represent the problem."
                    },
                    {
                      "expectationID": "1.OA.3",
                      "expectationDescription": "Apply properties of operations as strategies to add and subtract, e.g., using Commutative and Associative Properties of Addition."
                    },
                    {
                      "expectationID": "1.OA.4",
                      "expectationDescription": "Understand subtraction as an unknown-addend problem."
                    },
                    {
                      "expectationID": "1.OA.5",
                      "expectationDescription": "Relate counting to addition and subtraction, e.g., by counting on to add 2."
                    },
                    {
                      "expectationID": "1.OA.6",
                      "expectationDescription": "Add and subtract within 20, demonstrating fluency with various strategies for addition and subtraction within 10."
                    },
                    {
                      "expectationID": "1.OA.7",
                      "expectationDescription": "Understand the meaning of the equal sign, and determine if equations involving addition and subtraction are true or false."
                    },
                    {
                      "expectationID": "1.OA.8",
                      "expectationDescription": "Determine the unknown whole number in an addition or subtraction equation relating three whole numbers."
                    }
                  ]
                },
                {
                  "objectiveID": "1.NBT",
                  "objectiveTitle": "Number and Operations in Base Ten",
                  "objectiveDescription": "Extend the counting sequence and understand place value.",
                  "expectations": [
                    {
                      "expectationID": "1.NBT.1",
                      "expectationDescription": "Count to 120, starting at any number less than 120."
                    },
                    {
                      "expectationID": "1.NBT.2",
                      "expectationDescription": "Understand that the two digits of a two-digit number represent amounts of tens and ones."
                    },
                    {
                      "expectationID": "1.NBT.3",
                      "expectationDescription": "Compare two two-digit numbers based on meanings of the tens and ones digits."
                    },
                    {
                      "expectationID": "1.NBT.4",
                      "expectationDescription": "Add within 100, including adding a two-digit number and a multiple of 10, using strategies based on place value."
                    },
                    {
                      "expectationID": "1.NBT.5",
                      "expectationDescription": "Mentally find 10 more or 10 less than a two-digit number, without having to count."
                    },
                    {
                      "expectationID": "1.NBT.6",
                      "expectationDescription": "Subtract multiples of 10 in the range 10-90 from multiples of 10 in the range 10-90."
                    }
                  ]
                },
                {
                  "objectiveID": "1.MD",
                  "objectiveTitle": "Measurement and Data",
                  "objectiveDescription": "Measure lengths and work with time and money.",
                  "expectations": [
                    {
                      "expectationID": "1.MD.1",
                      "expectationDescription": "Order three objects by length; compare the lengths of two objects indirectly by using a third object."
                    },
                    {
                      "expectationID": "1.MD.2",
                      "expectationDescription": "Express the length of an object as a whole number of length units by laying multiple copies of a shorter object end to end."
                    },
                    {
                      "expectationID": "1.MD.3",
                      "expectationDescription": "Tell and write time in hours and half-hours using analog and digital clocks; identify pennies and dimes by name and value."
                    },
                    {
                      "expectationID": "1.MD.4",
                      "expectationDescription": "Organize, represent, and interpret data with up to three categories."
                    }
                  ]
                },
                {
                  "objectiveID": "1.G",
                  "objectiveTitle": "Geometry",
                  "objectiveDescription": "Reason with shapes and their attributes.",
                  "expectations": [
                    {
                      "expectationID": "1.G.1",
                      "expectationDescription": "Distinguish between defining attributes of shapes, such as being closed and three-sided."
                    },
                    {
                      "expectationID": "1.G.2",
                      "expectationDescription": "Compose two-dimensional and three-dimensional shapes to create a composite shape."
                    },
                    {
                      "expectationID": "1.G.3",
                      "expectationDescription": "Partition circles and rectangles into two and four equal shares, describing the shares using terms like halves and quarters."
                    }
                  ]
                }
